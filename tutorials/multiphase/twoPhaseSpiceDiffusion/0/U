FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    left
    {
        type           symmetryPlane;
    }

    right
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    down
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    up
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    hole
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }


    frontAndBack
    {
        type            fixedValue; //was "empty"
	value		uniform (0 0 0); //nothing was here
    }
}

