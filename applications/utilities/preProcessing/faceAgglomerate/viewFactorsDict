FoamFile {
    version 2.0;
    format ascii;
    class dictionary;
    object viewFactorsDict;
}
debug 1;

maxDynListLength    2147483647;

writeViewFactorMatrix     true;
writeFacesAgglomeration   true;
writePatchViewFactors     true;

nFacesInCoarsestLevel       10;
percentFacesInCoarsestLevel 0.0; //0.0 = disabled
featureAngle                10.0; //angle to identify geometric features and avoid agglomeration on those edges
featureAngleLevelsFactor    1.15; //factor to increase featureAngle for coarse level, angleCoarse = pow(angle, levelID)
featureAngleOnEveryLevel    true; //apply featureAngle strategy also on coarser level
agglomerateAllPatches       true; //if true agglomerate all patches without explicit list
debugMode                   false; //print debug info for testing

